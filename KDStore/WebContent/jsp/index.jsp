<?xml version="1.0" encoding="ISO-8859-1" ?>
<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
    pageEncoding="ISO-8859-1"%>
<!DOCTYPE html>
<html>
<head>
<title>POS System</title>

<style type="text/css">
#topButtons{
	text-align:center;
}

#mainButtons{
	text-align:center;
}
</style>

</head>
<body>

<div id="topButtons">
	<input type="button" value="Credit" name="Credit" />
	<input type="button" value="Need Assist" name="Need Assist" />
	<input type="button" value="Call For Help" name="Call For Help" />
</div>
<br />
<div id="mainButtons">
	<input type="button" value="Sale" name="sale" />
	<input type="button" value="Inquir" name="inquiry" /><br />
	<input type="button" value="Return" name="return" />
	<input type="button" value="No Sale" name="noSale" /><br />
	<input type="button" value="Post Sale" name="postSale" />
	<input type="button" value="Exchange" name="exchange" /><br />
	<input type="button" value="Payment" name="payment" />
	<input type="button" value="Customer Service" name="customerService" /><br />
	<input type="button" value="Blank" name="blank" />
	<input type="button" value="Login" name="login" /><br />
</div>

</body>
</html>